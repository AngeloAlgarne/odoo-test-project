<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="capitalization_share_tree_view" model="ir.ui.view">
            <field name="name">capitalization.share.tree.view</field>
            <field name="model">capitalization.share</field>
            <field name="arch" type="xml">
                <tree editable="bottom" create="1" delete="1">
                    <field name="name"/>
                    <field name="par_value"/>
                    <!--                    <field name="column_3">-->
                    <field name="authorized_no"/>
                    <field name="authorized_amount"/>
                    <!--                    <field name="column_4"/>-->
                    <field name="subscribed_no"/>
                    <field name="subscribed_amount"/>
                    <!--                    <field name="column_5"/>-->
                    <field name="treasury_no"/>
                    <field name="treasury_amount"/>
                    <!--                    <field name="column_6"/>-->
                    <field name="paid_up_no"/>
                    <field name="paid_up_amount"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>

