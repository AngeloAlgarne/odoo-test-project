<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="client_profile_tree_view" model="ir.ui.view">
            <field name="name">client.profile.tree.view</field>
            <field name="model">client.profile</field>
            <field name="arch" type="xml">
                <tree export_xlsx="0">
                    <field name="name"/>
                    <field name="client_system_generated"/>
                    <field name="user_id"/>
                    <field name="cluster_id"/>
                </tree>
            </field>
        </record>

        <record id="client_profile_form_view" model="ir.ui.view">
            <field name="name">client.profile.form.view</field>
            <field name="model">client.profile</field>
            <field name="arch" type="xml">
                <form create="false">
                    <header>
                        <button name="action_submit_supervisor" string="Submit" type="object" class="oe_highlight"
                                groups="amyu1.associate_group"
                                attrs="{'invisible': [('state', '!=', 'draft')]}"/>
                        <button name="action_approve_supervisor" string="Verify"
                                confirm="Are you sure you want to continue with your verification?" type="object"
                                class="oe_highlight"
                                groups="amyu1.supervisor_group"
                                attrs="{'invisible': [('state', '!=', 'supervisor')]}"/>
                        <button name="action_approve_manager" string="Approve"
                                confirm="Are you sure you want to continue with your approval?" type="object"
                                class="oe_highlight"
                                groups="amyu1.manager_group"
                                attrs="{'invisible': [('state', '!=', 'manager')]}"/>
                        <button name="action_return" string="Return" type="object"
                                groups="amyu1.supervisor_group"
                                attrs="{'invisible': [('state', '!=', 'supervisor')]}"/>
                        <button name="action_return" string="Return" type="object"
                                groups="amyu1.manager_group"
                                attrs="{'invisible': [('state', '!=', 'manager')]}"/>
                        <button name="action_return" string="Edit" type="object"
                                groups="amyu1.associate_group" confirm="Are you sure you want to edit?"
                                attrs="{'invisible': [('state', '!=', 'approved')]}"
                                class="oe_highlight"/>
                        <button name="draft_action"
                                string="Edit"
                                groups="amyu1.associate_group"
                                attrs="{'invisible': [('state', '!=', 'cancel')]}" type="object"/>
                        <!--                        <button name="draft_action" confirm="Are you sure you want to return this draft?"-->
                        <!--                                string="returned to Draft"-->
                        <!--                                groups="amyu1.supervisor_group"-->
                        <!--                                attrs="{'invisible': [('state', '!=', 'cancel')]}" type="object"/>-->
                        <!--                        <button name="draft_action" confirm="Are you sure you want to return this draft?"-->
                        <!--                                string="returned to Draft"-->
                        <!--                                groups="amyu1.manager_group"-->
                        <!--                                attrs="{'invisible': [('state', '!=', 'cancel')]}" type="object"/>-->

                        <field name="state" widget="statusbar"
                               status_invisible="draft,supervisor,manager,approved"/>
                    </header>
                    <sheet>
                        <div>
                            <field name="image_101" widget="image" nolabel="1" class="oe_avatar"
                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                        </div>
                        <div class="oe_title">
                            <h1>
                                <field name="name" placeholder="e.g. Lumber Inc"
                                       attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="client_system_generated" placeholder="System generated" readonly="1"
                                       attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="organization_type" options="{'no_open': True}"
                                       attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="nature_of_business" widget="textarea"
                                       attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                            </group>
                            <group>
                                <field name="date_of_engagement"
                                       attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="industry_class" options="{'no_open': True}"
                                       attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                            </group>
                        </group>

                        <notebook>
                            <page string="CONTACT">
                                <group>
                                    <group>
                                        <label for="registered_unit_no" string="Registered Address:"/>
                                        <div clas="o_address_format">
                                            <field name="registered_unit_no" placeholder="unit/floor..."
                                                   class="o_address_registered_unit_no"
                                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                            <field name="registered_building_name" placeholder="building name..."
                                                   class="o_address_registered_building_name"
                                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                            <field name="registered_street" placeholder="street..."
                                                   class="o_address_registered_street"
                                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                            <field name="registered_district" placeholder="district/barangay/village..."
                                                   class="o_address_registered_district"
                                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                            <field name="registered_city" placeholder="city..."
                                                   class="o_address_registered_city"
                                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                            <field name="registered_zip" placeholder="zip..."
                                                   class="o_address_registered_zip"
                                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        </div>
                                    </group>
                                    <group>
                                        <field name="registered_landline" placeholder="input 8 digit only"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="facsimile_no" placeholder="input 8 digit only"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="website"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                    </group>
                                </group>

                                <group string="Accounting or Office Admin Address">
                                    <group>
                                        <label for="office_admin_unit_no" string="Address:"/>
                                        <div clas="o_address_format">
                                            <field name="office_admin_unit_no" placeholder="unit/floor..."
                                                   class="o_address_office_admin_unit_no"
                                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                            <field name="office_admin_building_name" placeholder="building name..."
                                                   class="o_address_office_admin_building_name"
                                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                            <field name="office_admin_street" placeholder="street..."
                                                   class="o_address_registered_street"
                                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                            <field name="office_admin_district"
                                                   placeholder="district/barangay/village..."
                                                   class="o_address_office_admin_district"
                                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                            <field name="office_admin_city" placeholder="city..."
                                                   class="o_address_office_admin_city"
                                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                            <field name="office_admin_zip" placeholder="zip..."
                                                   class="o_address_office_admin_zip"
                                                   attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        </div>
                                    </group>
                                    <group>
                                        <field name="primary_contact_person"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="primary_contact_landline" placeholder="input only 8 digit"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="primary_contact_mobile"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="primary_contact_email"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="principal_accounting_officer"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="principal_accounting_landline" placeholder="input only 8 digit"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="principal_accounting_mobile"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="principal_accounting_email"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                    </group>
                                </group>
                            </page>

                            <page string="BIR REGISTRATION">
                                <group col="3">
                                    <group>
                                        <field name="vat" placeholder="input 9 Digit Only"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                    </group>
                                    <group>
                                        <field name="rdo_code"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                    </group>
                                    <group>
                                        <field name="registration_date" widget="date"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                    </group>
                                </group>
                                <group string="Tax Types" col="3">
                                    <group>
                                        <field name="income_tax" nolabel="1"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <label for="income_tax"/>
                                        <field name="value_added_tax" nolabel="1"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <label for="value_added_tax"/>
                                        <field name="other_percentage_tax" nolabel="1"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <label for="other_percentage_tax"/>
                                    </group>
                                    <group>
                                        <field name="withholding_tax_compensation" nolabel="1"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <label for="withholding_tax_compensation"/>
                                        <field name="withholding_tax_expanded" nolabel="1"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <label for="withholding_tax_expanded"/>
                                        <field name="withholding_tax_final" nolabel="1"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <label for="withholding_tax_final"/>
                                    </group>
                                    <group>
                                        <field name="excise_tax" nolabel="1"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <label for="excise_tax"/>
                                        <field name="registration_fee" nolabel="1"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <label for="registration_fee"/>
                                    </group>
                                </group>
                                <field name="other_percentage_tax_text" nolabel="1"
                                       attrs="{'invisible': [('other_percentage_tax', '=', False)]}"/>

                                <group col="2" string="">
                                    <group>
                                        <field name="taxpayer_type"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="invoice_tax"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="filing_payment"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                    </group>
                                    <group>
                                        <field name="books_of_account"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="psic_psoc"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="ll_cas_permit_no"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                    </group>
                                </group>
                                <p>
                                    Is the Company using any Point-of-Sale (POS),Cash Register (CRM),or Special
                                    Purpose (SPM) Machines?
                                    <field name="pos_crm_spm_yes_no" style="margin-left: 20px;" widget="radio"
                                           options="{'horizontal':True}"
                                           attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                </p>
                            </page>

                            <page string="SEC/DTI REGISTRATION">
                                <group>
                                    <group>
                                        <field name="registration_number"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                        <field name="trade_name"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                    </group>
                                    <group>
                                        <field name="registration_date_sec" widget="date"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                    </group>
                                </group>

                                <group string="Capitalization">
                                </group>
                                <field name="capital_sole_proprietor_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'sole_proprietor')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="capital_general_partner_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'general_partnership')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="capital_general_professional_partner_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'general_professional_partnership')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="capitalization_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'domestic_stock')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="capital_domestic_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'domestic_corp')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="capital_foreign_corp_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'foreign_corp')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="capital_foreign_nsnp_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'foreign_nsnp_corp')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="capital_roqh_foreign_corp_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'roqh_foreign_corp')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="capital_representative_office_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'representative_office')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <group col="3">
                                    <p>
                                        Annual Meeting
                                    </p>
                                    <group>
                                        <field name="date_per_law"
                                               attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                    </group>
                                </group>
                                <p>
                                    Is the Company a holder of a Secondary license issued by the Securities and
                                    Exchange
                                    Commission?
                                    <field name="sec_yes_no" style="margin-left: 20px;" widget="radio"
                                           options="{'horizontal':True}"
                                           nolabel="1"
                                           attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                    If Yes, what type of security is the Company permitted to sell?
                                </p>
                                <p>
                                    <field name="company_permit_yes" attrs="{'invisible': [('sec_yes_no', '=', 'no')]}"
                                           widget="text"
                                           nolabel="1"/>
                                </p>
                                <group string="Corporate Officers"/>
                                <field name="sole_proprietor_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'sole_proprietor')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="general_partnership_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'general_partnership')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="general_professional_partnership_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'general_professional_partnership')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="domestic_stock_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'domestic_stock')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="domestic_corp_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'domestic_corp')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="foreign_corp_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'foreign_corp')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="foreign_nsnp_corp_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'foreign_nsnp_corp')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="roqh_foreign_corp_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'roqh_foreign_corp')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                <field name="representative_office_ids"
                                       attrs="{'invisible': [('organization_type', '!=', 'representative_office')],'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                            </page>

                            <page name="regulatory_information" string="REGULATORY">
                                <p>
                                    Is the Entity regulated or accredited by or is a license holder issued by any of the
                                    following Government Regulatory Agencies?
                                    <field name="regulatory_yes_no" style="margin-left: 20px; margin-bottom:10px; "
                                           widget="radio"
                                           options="{'horizontal':True}"
                                           attrs="{'readonly': ['|','|','|', ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved'), ('state', '=', 'cancel')]}"/>
                                </p>
                                <group col="2">
                                    <group>
                                        <field name="bangko_sentral_pilipinas" nolabel="1"
                                               attrs="{'readonly':['|','|','|',('regulatory_yes_no', '=','no'),  ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved')]}"/>
                                        <label for="bangko_sentral_pilipinas"/>
                                        <field name="bureau_of_custom" nolabel="1"
                                               attrs="{'readonly':['|','|','|',('regulatory_yes_no', '=','no'),  ('state', '=', 'supervisor'), ('state', '=', 'manager'), ('state', '=', 'approved')]}"/>
