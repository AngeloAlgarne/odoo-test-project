<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="domestic_stock_tree_view" model="ir.ui.view">
            <field name="name">domestic.stock.tree.view</field>
            <field name="model">domestic.stock</field>
            <field name="arch" type="xml">
                <tree editable="bottom" create="1" delete="1">
                    <field name="name"/>
                    <field name="president"/>
                    <field name="treasurer"/>
                    <field name="corporate_secretary"/>
                    <field name="vice_chairman"/>
                    <field name="asst_treasurer"/>
                    <field name="asst_corp_secretary"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>